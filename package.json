{
  "name": "project-dep-view",
  "displayName": "project-dep-view",
  "description": "",
  "publisher": "evel-vscode-plugins",
  "icon": "image/help_panel_tool_fail.png",
  "version": "0.0.11",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "view-project-dep-source-code",
        "title": "项目依赖"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename==package.json",
          "command": "view-project-dep-source-code",
          "group": "project-dep"
        }
      ]
    },
    "configuration": {
      "title": "vscode-plugin-utils",
      "properties": {
        "vscode-plugin-utils.packageHandlerUtils": {
          "type": "string",
          "default": "npm",
          "description": "包管理工具",
          "enum": [
            "npm",
            "pnpm",
            "tnpm"
          ]
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-ai-by-qwen",
          "title": "code ai by qwen",
          "icon": "image/ai-code-logo-light.svg"
        }
      ]
    },
    "views": {
      "code-ai-by-qwen": [
        {
          "id": "code-ai-by-qwen-container",
          "type": "webview",
          "name": "code ai by qwen"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@types/which": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "fs-extra": "^11.2.0",
    "which": "^4.0.0"
  },
  "repository": "https://github.com/EvalGitHub/vscode-plugins-utils.git"
}
